package game.inheritance;

// 1. 뼈대가 되는 정보를 가진 Commoner 상속
public class Warrior extends Commoner {

	private int pDam;
	
	public void getInfo(){
		System.out.println("레벨 : " + lv + ", 체력 : " + hp + ", 마나 : " + mp + 
							", 물리데미지 : " + pDam + ", 이름 : " + name);
	}
	
	public Warrior(String name) {
		super(name);
	}
	
	

}
