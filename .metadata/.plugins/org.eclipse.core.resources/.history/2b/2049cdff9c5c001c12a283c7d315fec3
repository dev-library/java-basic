package thiskeyword;

public class Person {
	// 사람의 정보를 여기 기입합니다.
	private static String name;
	private int age;
	private int money;
	private String major;
	
	public Person(String name, int age, int money, String major) {
		Person.name = name;
		age = age;
		money = money;
		major = major;
	}
	
	
}
